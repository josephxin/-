.intelligent-analysis-center {
	position: absolute;
	top: 0.7rem;
	left: 9.6rem;
	transform: translate(-50%, 0);
	width: 8.2rem;
	height: 6.5rem;
}

.intelligent-analysis-content {
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	left: 0;
}

.intelligent-content-item {
	position: absolute;
	width: 2.41rem;
	height: 1.6rem;
	background: #000;
	opacity: 0;
	top: 0.38rem;
	left: -0.1rem;
	color: #f1f7ff;
	font-size: 0.18rem;
	box-sizing: border-box;
	padding-top: 0.43rem;
	line-height: 0.24rem;
	background: url("../../image/intelligent-analysis-center/intelligent-analysis-center-item-bg.png") no-repeat center;
	z-index: 10;
	background-size: 100% 100%;
}

.intelligent-content-item>p {
	display: flex;
	justify-content: center;
}

.intelligent-content-item-title {
	position: absolute;
	top: 0;
	left: 50%;
	font-size: 0.16rem;
	color: #f1f7ff;
	height: 0.32rem;
	line-height: 0.32rem;
	transform: translate(-50%, 0);
	font-weight: 900;
}

.intelligent-content-item span {
	float: left;
}

.activeIe, .linearColorFont1{
	background: none!important;
	color:#dae44c!important;
}


.intelligent-content-item0 {
	animation: transition0 300ms linear 500ms;
	animation-fill-mode: forwards;
}

@keyframes transition0 {
	0% {
		opacity: 0;
		transform: translate(121%, 110%);
	}
	100% {
		transform: translate(47%, 0%);
		opacity: 1;
	}
}


