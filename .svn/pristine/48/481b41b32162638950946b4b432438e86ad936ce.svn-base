.colorff9100 {
	color: #ff9100
}

.linear-color-font1 {
	font-weight: 900;
	max-width: 1.57rem;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	color: #dce673;
}

.linear-color-font2 {
	font-weight: 900;
	max-width: 1.57rem;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	color: #66ccff;
}

.marginR5 {
	margin-right: 0.05rem;
}

.marginL5 {
	margin-left: 0.05rem;
}

.intelligent-analysis-center-bg {
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	left: 0;
	background: url("../../image/intelligent-analysis-center/intelligent-analysis-center-bg.png") no-repeat;
	background-size: 100% 100%;
	z-index: 4;
}

.iac-content {
	width: 1.6rem;
	height: 0.9rem;
	position: absolute;
	top: 40%;
	left: 50%;
	transform: translate(-50%, -50%);
	/*cursor: pointer;*/
}

.iac-content p {
	height: 0.5rem;
	line-height: 0.5rem;
	letter-spacing: -0.08rem;
	text-align: center;
	text-indent: -0.1rem;
	color: white;
}

.iac-content p .iac-text-shadow {
	font-size: 0.5rem;
	font-weight: 900;
	background: linear-gradient(to bottom, #dae44c, #82d982);
	-webkit-background-clip: text;
	color: transparent;
	letter-spacing: -3px;
	padding-right: 3px;
}

.iac-content .text-btn {
	position: relative;
	margin: .1rem auto 0;
	width: 1.27rem;
	height: 0.3rem;
	line-height: 0.3rem;
	background-image: url('../../image/buttoned.png');
	background-size: 100% 100%;
}

.text-btn::after {
	content: '';
	display: block;
	width: 0.12rem;
	height: 0.07rem;
	background-image: url("../../image/more-arrow.png");
	background-size: 100%;
	position: absolute;
	top: 0.38rem;
	left: 0.58rem;
}

.iac-content p .unit {
	font-size: 0.20rem;
	margin-left: 0.1rem;
}

.iac-content p:last-child {
	font-size: 0.2rem;
	color: #ffffff;
	letter-spacing: 0;
	text-indent: 0;
	text-align: center;
	overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.iac-tab {
	width: 100%;
	height: 1.2rem;
	position: absolute;
	bottom: 0;
	left: 0;
	z-index: 5;
}

.iac-tab-button {
	height: 0.24rem;
	margin-bottom: 0.15rem;
}

.iac-tab-button div {
	width: 0.95rem;
	height: 0.24rem;
	line-height: 0.24rem;
	text-align: center;
	color: #f1f7ff;
	background: url("../../image/button.png") no-repeat center;
	cursor: pointer;
	margin-right: 0.2rem;
}

.iac-tab-button div:hover,
.iac-tab-button div.active {
	background: url("../../image/buttoned.png") no-repeat center;
}

.iac-content-msg {
	font-size: 0.16rem;
	color: #f1f7ff;
	line-height: 0.24rem;
	overflow-y: auto;
	height: 0.7rem;
	text-indent: 0.02rem;
}

.iac-content-msg span {
	display: inline;
}