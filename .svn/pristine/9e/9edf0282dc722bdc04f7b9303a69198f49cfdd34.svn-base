.page-test {
  width: 100%;
  height: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  align-items: center;
}

.page-test > div {
  border: 1px solid red;
  min-width: 420px;
  min-height: 280px;
}
